{
  "name": "tsx-serve",
  "version": "1.1.11",
  "description": "A simple static file server CLI tool.",
  "main": "./dist/tsx-serve.cjs",
  "module": "./dist/tsx-serve.mjs",
  "types": "./dist/tsx-serve.d.cts",
  "exports": {
    "require": {
      "types": "./dist/tsx-serve.d.cts",
      "default": "./dist/tsx-serve.cjs"
    },
    "import": {
      "types": "./dist/tsx-serve.d.mts",
      "default": "./dist/tsx-serve.mjs"
    }
  },
  "bin": {
    "tsx-serve": "./dist/tsx-serve.js"
  },
  "scripts": {
    "build": "pkgroll"
  },
  "dependencies": {
    "express": "^4.21.0",
    "localtunnel": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/localtunnel": "^2.0.4",
    "@types/node": "^16.11.1",
    "pkgroll": "^2.5.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
